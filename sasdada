--rex script hub
print("rex on top")

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "sigma rex script hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local iy = Window:MakeTab({
	Name = "Infinite Yield",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

iy:AddButton({
	Name = "Run Script",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
  	end    
})

